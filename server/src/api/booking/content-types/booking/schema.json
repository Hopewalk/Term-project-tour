{
  "kind": "collectionType",
  "collectionName": "bookings",
  "info": {
    "singularName": "booking",
    "pluralName": "bookings",
    "displayName": "Booking",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "booking_date": {
      "type": "datetime"
    },
    "total_price": {
      "type": "decimal"
    },
    "booking_status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "confirmed",
        "cancelled"
      ],
      "default": "pending"
    },
    "payment_status": {
      "type": "enumeration",
      "enum": [
        "paid",
        "unpaid"
      ],
      "default": "unpaid"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "bookings"
    },
    "tour": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::tour.tour",
      "inversedBy": "bookings"
    },
    "payment": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::payment.payment",
      "inversedBy": "bookings"
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "line_id": {
      "type": "string"
    },
    "participant": {
      "type": "integer"
    }
  }
}
